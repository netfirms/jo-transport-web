{
  "name": "jo-transport-web",
  "version": "1.0.0",
  "description": "Website for J.O Transportation and service Co..Ltd.",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:tailwind && vite build",
    "preview": "vite preview",
    "build:tailwind": "tailwindcss -i ./css/tailwind.css -o ./css/tailwind.output.css --minify",
    "watch:tailwind": "tailwindcss -i ./css/tailwind.css -o ./css/tailwind.output.css --watch",
    "dev:all": "concurrently \"npm run watch:tailwind\" \"npm run dev\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "keywords": [
    "transportation",
    "limousine",
    "airport",
    "bangkok"
  ],
  "author": "J.O Transportation and service Co..Ltd.",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.5.0",
    "concurrently": "^8.2.2",
    "cypress": "^12.14.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.5"
  }
}
